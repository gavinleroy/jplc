
fn some_long_name(x : int, y : int, z : int) : int {
   let a = x + y;
   let b = x - y;
   let c = x + z;
   let d = x - z;
   let e = y + z;
   let f = y - z;
   let g = a * b;
   let h = b * c;
   let i = c * d;
   let j = d * e;
   let k = f / h;
   let l = h / i;
   let m = i / j;
   let n = j / k;
   return a + b + c + d + e + f + g + h + i + j + k + l + m + n;
}

